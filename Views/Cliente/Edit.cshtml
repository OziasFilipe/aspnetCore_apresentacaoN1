            </div>
            <div class="form-group">
                <label asp-for="Inclusao" class="control-label"></label>
                <input asp-for="Inclusao" class="form-control" />
                <span asp-validation-for="Inclusao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="Alteracao" class="control-label"></label>
                <input asp-for="Alteracao" class="form-control" />
                <span asp-validation-for="Alteracao" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Save" class="btn btn-default" />
            </div>
        </form>
    </div>
</div>



@section Scripts {
    
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
